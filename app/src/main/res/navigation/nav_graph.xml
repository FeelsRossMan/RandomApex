<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/HomeFragment">

    <fragment
        android:id="@+id/RandomFragment"
        android:name="com.evanross.randomapex.RandomFragment"
        android:label="random_fragment_label"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_RandomFragment_to_HomeFragment"
            app:destination="@id/HomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.evanross.randomapex.HomeFragment"
        android:label="home_fragment_label"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_HomeFragment_to_RandomFragment"
            app:destination="@id/RandomFragment" />
    </fragment>
</navigation>